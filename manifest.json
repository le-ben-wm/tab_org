{
  "manifest_version": 2,

  "name": "Tab Organizer",
  "description": "Organizes tabs in Google Chrome",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Tab Organizer"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
